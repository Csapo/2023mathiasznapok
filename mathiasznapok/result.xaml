<Window x:Class="mathiasznapok.result"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mathiasznapok"
        mc:Ignorable="d"
        Title="result" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/results.jpg"/>
        </Grid.Background>
        <Label Content="Írd id a neved:" HorizontalAlignment="Left" Margin="114,302,0,0" VerticalAlignment="Top" Width="175" FontFamily="Rosewood Std Regular" FontSize="24"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="114,349,0,0" VerticalAlignment="Top" Width="150" Height="61" FontFamily="Rosewood Std Regular" FontSize="36">
            <Button.Background>
                <ImageBrush/>
            </Button.Background>
        </Button>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="330,300,49,14">
            <ListBox.Background>
                <ImageBrush/>
            </ListBox.Background>
        </ListBox>

    </Grid>
</Window>
